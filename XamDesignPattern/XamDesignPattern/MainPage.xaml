<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XamDesignPattern.MainPage">

    <StackLayout>
        <Label Text="Design Pattern" HorizontalTextAlignment="Center" VerticalTextAlignment="Center"/>
        <Button Text="Adaptor" Clicked="Button_Clicked"/>
        <Button Text="Bridge" Clicked="Button_Clicked_1"/>
        <Button Text="Composite" Clicked="Button_Clicked_2"/>
        <Button Text="Decorator" Clicked="Button_Clicked_3"/>
        <Button Text="Factory" Clicked="Button_Clicked_4"/>
        <Button Text="Memento" Clicked="Button_Clicked_5"/>
        <Button Text="Observer" Clicked="Button_Clicked_6"/>
        <Button Text="Prototype" Clicked="Button_Clicked_7"/>
        <Button Text="Singleton" Clicked="Button_Clicked_8"/>
        <Button Text="Strategy" Clicked="Button_Clicked_9"/>
    </StackLayout>

</ContentPage>
